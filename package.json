{
  "name": "node-api-template",
  "version": "1.0.0",
  "description": "Node API Template",
  "main": "app.js",
  "scripts": {
    "start": "nodemon ./src/app.js --exec babel-node --presets es2015",
    "pretest": "yarn migrate:test && yarn seed:test",
    "test": "NODE_ENV=test PG_OUTPUT_OFF=true API_ERRORS_OFF=true mocha --recursive --compilers js:babel-register",
    "lint": "eslint .",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "migrate:test": "sequelize db:migrate --env test",
    "seed:test": "sequelize db:seed:all --env test"
  },
  "author": "Tyler Petresky <hello@tylerpetresky.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.0",
    "connect-pg-simple": "^3.1.2",
    "cors": "^2.8.1",
    "crypto": "^0.0.3",
    "ejs": "^2.5.5",
    "express": "^4.14.0",
    "express-session": "^1.15.0",
    "express-validator": "^3.1.2",
    "jquery": "^3.1.1",
    "jwt-simple": "^0.5.1",
    "knex": "^0.12.6",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-stub": "^1.1.1",
    "pg": "^6.1.2",
    "sequelize": "^3.30.0",
    "standard": "^8.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "colors": "^1.1.2",
    "eslint": "^3.14.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "sequelize-cli": "^2.5.1"
  }
}
